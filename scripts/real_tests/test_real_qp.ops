stream("ctrl.in_desiredTaskSpace_port", ros.topic("/cart/traj_setpoint"))

stream("KinDyn.out_cartPos_0_port", ros.topic("/robot/fdb/cart_pose_0"))
ctrl.setCartDamping(9)
ctrl.setCartStiffness(300)

